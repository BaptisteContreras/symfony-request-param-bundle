{
  "name": "baptiste-contreras/symfony-request-param-bundle",
  "description": "Symfony bundle to be able to use object request parameter",
  "type": "symfony-bundle",
  "license": "MIT",
  "minimum-stability": "stable",
  "require": {
    "php": ">=8.1",
    "symfony/config":  "~4.4 || ^5.0 || ^6.0",
    "symfony/dependency-injection": "~4.4 || ^5.0 || ^6.0",
    "sensio/framework-extra-bundle": "~4.4 || ^5.0 || ^6.0",
    "symfony/http-kernel":  "~4.4 || ^5.0 || ^6.0",
    "symfony/serializer": "~4.4 || ^5.0 || ^6.0",
    "symfony/serializer-pack": "^v1.1.0",
    "symfony/validator": "~4.4 || ^5.0 || ^6.0"
  },
  "conflict": {
    "ext-swoole": "<4.6.0"
  },
  "autoload": {
    "psr-4": {
      "BaptisteContreras\\SymfonyRequestParamBundle\\": "src/"
    }
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "v3.12.0"
  }
}
